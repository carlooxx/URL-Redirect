{
  "manifest_version": 3,
  "name": "Url Redirect",
  "version": "1.0",
  "description": "Redirects URLs according to configured rules",
  "permissions": ["storage", "tabs", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "js/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "devtools_page": "devtools.html"
}
